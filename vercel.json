{
  "git": {
    "deploymentEnabled": {
      "main": false,
      "staging": true
    }
  },
  "github": {
    "autoDeployment": false
  },
  "env": {
    "VITE_ENVIRONMENT": "production"
  },
  "build": {
    "env": {
      "VITE_SUPABASE_URL": "@supabase-url-prod",
      "VITE_SUPABASE_ANON_KEY": "@supabase-anon-key-prod",
      "VITE_GOOGLE_MAPS_API_KEY": "@google-maps-api-key-prod"
    }
  },
  "preview": {
    "env": {
      "VITE_ENVIRONMENT": "staging",
      "VITE_SUPABASE_URL": "@supabase-url-staging", 
      "VITE_SUPABASE_ANON_KEY": "@supabase-anon-key-staging",
      "VITE_GOOGLE_MAPS_API_KEY": "@google-maps-api-key-staging"
    }
  }
}